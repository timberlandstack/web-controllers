{
  "name": "@timberland/web-controllers",
  "version": "0.0.10",
  "type": "module",
  "license": "MIT",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./elements": "./src/customElements/index.js",
    "./helpers": "./src/helpers/index.js"
  },
  "author": "Álvaro Palma Mancheño",
  "keywords": [
    "web components"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timberlandstack/web-controllers.git"
  },
  "description": "Lightweight alternative to jQuery and stimulus",
  "scripts": {
    "test": "vitest",
    "fmt": "biome format --write",
    "lint": "biome lint",
    "cs": "changeset",
    "version": "changeset version",
    "version:test": "changeset version --snapshot charmander",
    "release": "pnpm run minify && changeset publish",
    "release:test": "changeset publish --no-git-tag --tag charmander",
    "minify": "node ./scripts/minify.js"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/cli": "^2.28.1",
    "esbuild": "^0.25.2",
    "jsdom": "^26.0.0",
    "vitest": "^3.0.9"
  },
  "files": [
    "src/",
    "README.md",
    "dist/",
    "!src/__tests__/",
    "!**/*.test.js",
    "!src/_barrel.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.0.6+sha1.648f6014eb363abb36618f2ba59282a9eeb3e879",
  "dependencies": {
    "@timberland/emitters": "^0.0.4"
  }
}
